{
  "name": "oot-bingo-generator",
  "version": "2.0.0",
  "description": "Generator for OoT Bingo",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "start": "ts-node src/sandbox/main.ts",
    "test": "jest",
    "bundle": "npx webpack --config webpack.config.bundle.js",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint tsconfig.json",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ootbingo/oot-bingo-generator.git"
  },
  "keywords": [
    "oot",
    "bingo",
    "generator",
    "speedrun"
  ],
  "author": "",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/ootbingo/oot-bingo-generator/issues"
  },
  "homepage": "https://github.com/ootbingo/oot-bingo-generator#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@types/eslint": "^8.4.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2
  }
}
